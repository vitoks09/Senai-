        "delta = b**2 - 4*a*c\n",
        "raiz1 = (-b + math.sqrt(delta) )/(2*a)\n",
        "raiz2 = (-b - math.sqrt(delta) )/(2*a)\n",
        "print(\"raiz 1  = \", raiz1, \"Raiz2 = \", raiz2 )"
